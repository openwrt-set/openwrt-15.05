/dts-v1/;

/include/ "mt7620a.dtsi"

/ {
	compatible = "ralink,irz_mt02", "ralink,mt7620a-soc";
	model = "s-terra";
	chosen {
	    bootargs = "console=ttyS0,57600";
	};
	palmbus@10000000 {
		i2c@900 {
			status = "okay";
			rtc@51 {
				compatible = "nxp,pcf85063";
				reg = <0x51>;
			};
		};

		spi@b00 {
			status = "okay";

			m25p80@0 {
				#address-cells = <1>;
				#size-cells = <1>;
				compatible = "w25q128";
				reg = <0 0>;
				linux,modalias = "m25p80", "w25q128";
				spi-max-frequency = <10000000>;

				partition@0 {
					label = "u-boot";
					reg = <0x0 0x30000>;
				};

				partition@30000 {
					label = "u-boot-env";
					reg = <0x30000 0x10000>;
				};

				factory: partition@40000 {
					label = "factory";
					reg = <0x40000 0x10000>;
				};
				partition@50000 {
					label = "firmware";
					reg = <0x50000 0xbb0000>;
				};
				partition {
					label = "opt";
					reg = <0xc00000 0x400000>;
				};
			};
		};

	    	gpio@600 {
			status = "okay";
		};
		gpio@638 {
			status = "okay";
		};
		gpio@660 {
			status = "okay";
		};
		gpio@688 {
			status = "okay";
		};
	};

	pinctrl {
		state_default: pinctrl0 {
			gpio {
				ralink,group = "rgmii1", "rgmii2", "wled";
				ralink,function = "gpio";
			};
			sd {
				ralink,group = "nd_sd";
				ralink,function = "nand";
			};
		};
		uartf_pins: uartf {
			uartf {
				ralink,group = "uartf";
				ralink,function = "uartf";
			};
		};
	};

	ethernet@10100000 {
		status = "okay";

		pinctrl-names = "default";
		pinctrl-0 = <&mdio_pins>;

		ralink,port-map = "llll";
		mtd-mac-address = <&factory 0x28>;

		port@4 {
			status = "okay";
			phy-mode = "ephy";
			phy-handle = <&phy4>;
		};

		mdio-bus {
			status = "okay";

			phy4: ethernet-phy@4 {
				reg = <4>;
				phy-mode = "ephy";
			};
		};
	};



	gsw@10110000 {
		ralink,port4 = "ephy";
	};

	pcie@10140000 {
		status = "okay";
	};

	gpio {
		compatible = "gpio-export";
		USB1_POWER {
		    gpios = <&gpio0 0 1>;
		    gpio-export,name  = "usb1:power";
		    gpio-export,input;
		    gpio-export,direction-may-change;
		};
		USB2_POWER {
		    gpios = <&gpio1 6 1>;
		    gpio-export,name  = "usb2:power";
		    gpio-export,output = <1>;
		    gpio-export,direction-may-change;
		};
		STATUS_RESET {
		    gpios = <&gpio1 9 1>;
		    gpio-export,name  = "status:reset";
		    gpio-export,input;
		};
	};
	gpio-leds {
		compatible = "gpio-leds";
		STATUS {
			label = "status";
			gpios = <&gpio1 0 0>;
		};
		CONNECT_GREEN {
			label = "user:green";
			gpios = <&gpio1 1 0>;
		};
		CONNECT_RED {
			label = "user:red";
			gpios = <&gpio1 2 0>;
		};
	};
	ehci@101c0000 {
		status = "okay";
	};

	ohci@101c1000 {
		status = "okay";
	};
	wmac@10180000 {
		status = "okay";
		ralink,mtd-eeprom = <&factory 0>;
	};
	nand {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible="mtk,mt7620-nand";
	};
};

